{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Graphic Library Documentation",
  "entryPoints": ["2d", "3d", "dom", "interfaces", "mixins", "model", "utils"],
  "exclude": ["demo"],
  "entryPointStrategy": "expand",
  "readme": "README.md",
  "out": "doc",
  "theme": "hierarchy",
  "customCss": "./document/themes/typedoc.css",
  "externalPattern": ["**/node_modules/**/*"],
  "includeVersion": true,
  "excludeExternals": true,
  "cleanOutputDir": true,
  "plugin": ["typedoc-plugin-missing-exports", "typedoc-theme-hierarchy"]
}
